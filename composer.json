{
    "name": "ghostzero/maid",
    "description": "Cleanup tool for vendor and node_modules directories.",
    "keywords": [
        "maid",
        "composer",
        "node_modules",
        "cleanup"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "René Preuß",
            "email": "rene@preuss.io"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-sodium": "*"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "jolicode/jolinotif": "^2.2",
        "laravel-zero/framework": "^8.9",
        "mbaynton/launch-cli-editor": "^1.0",
        "padraic/phar-updater": "^1.0.6",
        "symfony/yaml": "^6.0",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "scripts": {
        "build": "@php maid app:build maid --build-version=dev-master && git commit -am 'build: new dev version' && ggp"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/maid"
    ]
}
